<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainGUIController">
   <children>
      <ToolBar>
        <items>
            <Button fx:id="openButton" mnemonicParsing="false" onAction="#openButtonPressed" text="Сохранить" />
            <Button fx:id="loadButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#loadButtonPressed" text="Загрузить" />
            <Pane HBox.hgrow="ALWAYS" />
            <ToggleButton fx:id="SettingsButton" alignment="CENTER_LEFT" mnemonicParsing="false" onAction="#toggleSettings" text="Настройки" />
        </items>
      </ToolBar>
       <SplitPane fx:id="splitPane" focusTraversable="true" prefHeight="160.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
